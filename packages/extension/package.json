{
  "name": "code-journey",
  "displayName": "Code Journey",
  "publisher": "himeditator",
  "icon": "assets/icons/code-journey.png",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiMeditator/code-journey"
  },
  "homepage": "https://github.com/HiMeditator/code-journey",
  "bugs": {
    "url": "https://github.com/HiMeditator/code-journey/issues"
  },
  "qna": "https://github.com/HiMeditator/code-journey/issues",
  "license": "MIT",
  "engines": {
    "vscode": "^1.80.0"
  },
  "galleryBanner": {
    "color": "#1A1A1A",
    "theme": "dark"
  },
  "categories": [
    "Visualization",
    "Data Science"
  ],
  "keywords": [
    "Log",
    "Statistics"
  ],
  "activationEvents": [],
  "main": "./dist/extension/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "code-journey",
          "title": "Code Journey",
          "icon": "assets/icons/code-journey.svg"
        }
      ]
    },
    "views": {
      "code-journey": [
        {
          "type": "webview",
          "id": "code-journey.sidebar",
          "name": "Code Journey",
          "icon": "assets/icons/code-journey.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
